    <script>
        $(document).ready(function(){
            const lang =`@Context.GetCurrentCulture()`;
            const selectedLangText= $(`[data-lang="${lang}"`).text();
            $(`[data-lang-viewer="#language"]`).text(selectedLangText);
              $('[data-lang]').click(function (e) {
            e.preventDefault();
            let path = `${window.location.pathname}${window.location.search}`;
            let selectedLang = $(e.currentTarget).data('lang');
            var result = /\/(en|az|ru)?\/?(.*)?/.exec(path);
            if (result != null && result[2] != undefined) {
                 path = `/${selectedLang}/${result[2]}`;
            }
            else if (result != null) {
                path = `/${selectedLang}`;
            }           
            window.location.href = path; 
        });
        });
    </script>
